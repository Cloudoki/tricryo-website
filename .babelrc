{
  "presets": [
    ["@babel/preset-env", {
      "useBuiltIns": "usage",
      "loose": true,
      "modules": false,
      "forceAllTransforms": true
    }],
    "@babel/preset-react"
  ],
  "env": {
    "test": {
      "presets": [["@babel/env"], "@babel/preset-react"]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel",
    "add-react-displayname"
  ]
}
